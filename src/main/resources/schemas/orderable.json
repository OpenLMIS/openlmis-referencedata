{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Orderable",
  "description": "A single orderable",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "productCode": {
      "type": "string",
      "title": "productCode"
    },
    "fullProductName": {
      "type": "string",
      "title": "fullProductName"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "netContent": {
      "type": "number",
      "title": "netContent"
    },
    "packRoundingThreshold": {
      "type": "integer",
      "title": "packRoundingThreshold"
    },
    "roundToZero": {
      "type": "boolean",
      "title": "roundToZero"
    },
    "children": {
      "type": "array",
      "title": "children",
      "items": {
        "type": "object",
        "$ref": "orderableChildDto.json"
      }
    },
    "programs": {
      "type": "array",
      "title": "programs",
      "items": {
        "type": "object",
        "$ref": "programOrderableDto.json"
      },
      "uniqueItems": true
    },
    "identifiers": {
      "type": "object",
      "title": "identifiers"
    },
    "extraData": {
      "type": "object",
      "title": "extraData"
    },
    "meta": {
      "type": "object",
      "$ref": "metadata.json",
      "description": "The orderable metadata"
    },
    "quarantined": {
      "type": "boolean",
      "title": "quarantined"
    }
  },
  "required": [
    "productCode",
    "packRoundingThreshold",
    "roundToZero",
    "netContent"
  ]
}
