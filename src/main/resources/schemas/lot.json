{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Lot",
  "description": "A single lot",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "lotCode": {
      "type": "string",
      "title": "lotCode"
    },
    "expirationDate": {
      "type": "string",
      "title": "expirationDate"
    },
    "manufactureDate": {
      "type": "string",
      "title": "manufactureDate"
    },
    "tradeItem": {
      "type": "object",
      "title": "tradeItem"
    },
    "active": {
      "type": "boolean",
      "title": "active"
    },
    "quarantined": {
      "type": "boolean",
      "title": "quarantined"
    }
  },
  "required": [
    "lotCode",
    "tradeItem"
  ]
}