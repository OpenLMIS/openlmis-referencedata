{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProgramOrderable",
  "description": "A single programOrderable",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "program": {
      "type": "object",
      "$ref": "program.json",
      "title": "program"
    },
    "product": {
      "type": "object",
      "$ref": "orderable.json",
      "title": "product"
    },
    "dosesPerMonth": {
      "type": "integer",
      "title": "dosesPerMonth"
    },
    "active": {
      "type": "boolean",
      "title": "active"
    },
    "orderableDisplayCategory": {
      "type": "object",
      "$ref": "orderableDisplayCategory.json",
      "title": "orderableDisplayCategory"
    },
    "fullSupply": {
      "type": "boolean",
      "title": "fullSupply"
    },
    "displayOrder": {
      "type": "integer",
      "title": "displayOrder"
    },
    "maxMonthsStock": {
      "type": "integer",
      "title": "maxMonthsStock"
    },
    "pricePerPack": {
      "type": "number",
      "title": "pricePerPack"
    }
  },
  "required": ["id", "program", "product", "dosesPerMonth", "active", "orderableDisplayCategory",
    "fullSupply"]
}