  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProgramOrderableDto",
  "description": "A single product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "orderableId": {
      "type": "string",
      "title": "orderableId"
    },
    "orderableName": {
      "type": "string",
      "title": "orderableName"
    },
    "orderableCode": {
      "type": "string",
      "title": "orderableCode"
    },
    "orderablePackSize": {
      "type": "number",
      "title": "orderablePackSize"
    },
    "orderableDisplayCategoryId": {
      "type": "string",
