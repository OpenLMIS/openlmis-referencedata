{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Processing Period",
  "description": "A single processingPeriod",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "processingSchedule": {
      "type": "object",
      "$ref": "processingSchedule.json"
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "startDate": {
      "type": "string",
      "title": "startDate"
    },
    "endDate": {
      "type": "string",
      "title": "endDate"
    },
    "durationInMonths": {
      "type": "integer",
      "title": "durationInMonths"
    }
  },
  "required": [
    "processingSchedule", "name", "startDate", "endDate"
  ]
}